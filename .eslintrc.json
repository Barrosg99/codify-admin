{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
          "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
				"react/jsx-props-no-spreading": ["off"],
				"no-tabs": "off",
				"no-param-reassign": "off",
				"no-console": "off",
				"react/prop-types": [0],
				"react/destructuring-assignment": [0, "never"]
    }
}
